<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<Pane prefHeight="215.0" prefWidth="356.0" stylesheets="@../../common.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.spectralogic.dsbrowser.gui.components.jobpriority.JobSettingsPresenter">
    <children>
        <GridPane alignment="CENTER" prefHeight="215.0" prefWidth="357.0" vgap="10.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="295.0" minWidth="10.0" prefWidth="227.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="386.0" minWidth="10.0" prefWidth="225.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints />
            <RowConstraints />
            </rowConstraints>
            <children>
                <Label alignment="TOP_LEFT" text="Put Job Priority:" GridPane.rowIndex="1" />
                <Label text="Get Job Priority:" GridPane.rowIndex="3" />
                <ComboBox fx:id="putJobPriority" prefHeight="25.0" prefWidth="108.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets />
                    </GridPane.margin>
                </ComboBox>
                <ComboBox fx:id="getJobPriority" prefHeight="25.0" prefWidth="109.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
                    <GridPane.margin>
                        <Insets />
                    </GridPane.margin>
                </ComboBox>
                <!--<TextField fx:id="getJobPriority" GridPane.columnIndex="1" GridPane.rowIndex="3"/>-->
                <!--<TextField fx:id="logDirectory" onAction="#showFileExplorer" onMouseClicked="#showFileExplorer" GridPane.columnIndex="1" />-->
                <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="15.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
                    <children>
                        <Button fx:id="saveSettingsButton" mnemonicParsing="false" onAction="#saveJobSettings" text="Save" />
                        <Button fx:id="cancelSettingsButton" cancelButton="true" mnemonicParsing="false" onAction="#closeDialog" text="Cancel" />
                    </children>
                    <GridPane.margin>
                        <Insets top="10.0" />
                    </GridPane.margin>
                </HBox>
                <!--<Label text="Enable Logging:" GridPane.rowIndex="3" />
                <CheckBox fx:id="debugLogging" mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="3" />-->
            </children>
            <padding>
                <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
            </padding>
        </GridPane>
    </children>
</Pane>
