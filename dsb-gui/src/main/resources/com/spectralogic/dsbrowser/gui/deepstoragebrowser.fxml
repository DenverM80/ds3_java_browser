<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.spectralogic.dsbrowser.gui.DeepStorageBrowserPresenter">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" fx:id="fileMenu">
            <items>
                  <MenuItem mnemonicParsing="false" onAction="#showSessionPopup" fx:id="sessionsMenuItem"/>
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" onAction="#showSettingsPopup" fx:id="settingsMenuItem" />
                  <SeparatorMenuItem mnemonicParsing="false" />
              <MenuItem mnemonicParsing="false" onAction="#closeWindow" fx:id="closeMenuItem" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" fx:id="viewMenu">
               <items>
                  <CheckMenuItem fx:id="jobsMenuItem" mnemonicParsing="false" />
                  <!--
                  <Menu mnemonicParsing="false" fx:id="themeMenuItem">
                    <items>
                        <CheckMenuItem mnemonicParsing="false" selected="true" fx:id="lightViewCheckMenuItem" />
                        <CheckMenuItem mnemonicParsing="false" fx:id="darkViewCheckMenuItem"/>
                    </items>
                  </Menu>
                  -->
               </items>
            </Menu>
          <Menu mnemonicParsing="false" fx:id="helpMenu">
            <items>
            <!--
                  <MenuItem mnemonicParsing="false" fx:id="helpMenuItem" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  -->
              <MenuItem mnemonicParsing="false" onAction="#showAboutPopup" fx:id="aboutMenuItem" />
              <!--
                <MenuItem mnemonicParsing="false" onAction="#showLicensePopup" fx:id="licenseMenuItem" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" fx:id="versionMenuItem" />
                  -->
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <center>
      <SplitPane fx:id="jobSplitter" orientation="VERTICAL">
         <items>
         <SplitPane dividerPositions="0.5" BorderPane.alignment="CENTER">
           <items>
             <AnchorPane fx:id="fileSystem" minHeight="0.0" minWidth="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />

                         <AnchorPane fx:id="blackPearl" minHeight="0.0" minWidth="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />

                 </items>
         </SplitPane>
         </items>
      </SplitPane>
   </center>
   <bottom>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <Label text="Label" />
         </children>
      </HBox>
   </bottom>
