<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.cell.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox prefWidth="286.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.spectralogic.dsbrowser.gui.components.localfiletreetable.LocalFileTreeTablePresenter">
 <children>
    <ToolBar prefHeight="40.0" prefWidth="200.0">
      <items>
        <Button fx:id="homeButton">
            <tooltip>
                <Tooltip text="Home" />
            </tooltip>
        </Button>
          <Button fx:id="refreshButton">
              <tooltip>
                  <Tooltip text="Refresh" />
              </tooltip>
          </Button>

      </items>
    </ToolBar>
     <TreeTableView fx:id="treeTable" layoutX="24.0" layoutY="33.0" prefWidth="296.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" VBox.vgrow="ALWAYS">
    <columns>
        <TreeTableColumn minWidth="100.0" prefWidth="200.0" text="Name">
            <cellValueFactory>
                <TreeItemPropertyValueFactory property="name" />
            </cellValueFactory>
        </TreeTableColumn>
        <TreeTableColumn prefWidth="100.0" text="Type">
          <cellValueFactory>
              <TreeItemPropertyValueFactory property="type" />
          </cellValueFactory>
        </TreeTableColumn>
        <TreeTableColumn prefWidth="100.0" text="Size">
          <cellValueFactory>
              <TreeItemPropertyValueFactory property="size" />
          </cellValueFactory>
        </TreeTableColumn>
        <TreeTableColumn prefWidth="100.0" text="Date Modified">
          <cellValueFactory>
              <TreeItemPropertyValueFactory property="lastModified" />
          </cellValueFactory>
        </TreeTableColumn>
    </columns>
         <columnResizePolicy>
            <TreeTableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
</TreeTableView>

 </children>
</VBox>
